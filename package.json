{
  "name": "true-compass",
  "displayName": "True Compass",
  "version": "1.0.0",
  "description": "A precise, ad-free compass app using device magnetometer with tilt compensation",
  "main": "expo-router/entry",
  "type": "module",
  "author": "True Compass Team",
  "license": "UNLICENSED",
  "keywords": [
    "compass",
    "navigation",
    "magnetometer",
    "direction",
    "heading",
    "tilt-compensation"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "deploy": "npx tsx --tsconfig cli/tsconfig.json cli/index.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.12",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-keep-awake": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-sensors": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "ink": "^6.3.1",
    "ink-big-text": "^2.0.0",
    "ink-box": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
